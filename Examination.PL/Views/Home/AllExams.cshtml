@model IEnumerable<Examination.BL.ViewModels.ExamVM>

@{
    ViewData["Title"] = "Select Exam";
}

<h1>Select Exam</h1>


<table class="table">
    <thead>
        <tr>
            <th> Id </th>
            <th>Subject</th>

            <th>Take The Exam</th>
            

        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Subject</td>


                <td><a class="btn btn-primary" asp-controller="Home" asp-action="Index" asp-route-id="@item.Id">Take The Exam</a></td>
                
            </tr>
        }
    </tbody>
</table>
